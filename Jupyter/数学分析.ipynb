   "source": [
    "import math\n",
    "n,k = 16,2\n",
    "s = 0\n",
    "for j in range(k+1):\n",
    "    s += (-1)**(k-j) * math.comb(k, j) * j**n\n",
    "\n",
    "(1/math.factorial(k))*s"
   ]
